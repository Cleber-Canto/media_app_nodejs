
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id @default(uuid())
  email     String    @unique
  password  String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}

model Item {
  id          String   @id @default(uuid())
  title       String
  artist      String
  album       String
  coverURL    String
  description String
  imageUrl    String
  audioUrl    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}